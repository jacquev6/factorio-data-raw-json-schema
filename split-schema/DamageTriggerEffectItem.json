{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/DamageTriggerEffectItem.html",
  "type": "object",
  "properties": {
    "repeat_count": {
      "$ref": "../split-schema/double.json"
    },
    "repeat_count_deviation": {
      "$ref": "../split-schema/uint16.json"
    },
    "probability": {
      "$ref": "../split-schema/float.json"
    },
    "affects_target": {
      "$ref": "../split-schema/bool.json"
    },
    "show_in_tooltip": {
      "$ref": "../split-schema/bool.json"
    },
    "damage_type_filters": {
      "$ref": "../split-schema/DamageTypeFilters.json"
    },
    "type": {
      "type": "string",
      "const": "damage"
    },
    "damage": {
      "$ref": "../split-schema/DamageParameters.json"
    },
    "apply_damage_to_trees": {
      "$ref": "../split-schema/bool.json"
    },
    "vaporize": {
      "$ref": "../split-schema/bool.json"
    },
    "use_substitute": {
      "$ref": "../split-schema/bool.json"
    },
    "lower_distance_threshold": {
      "$ref": "../split-schema/uint16.json"
    },
    "upper_distance_threshold": {
      "$ref": "../split-schema/uint16.json"
    },
    "lower_damage_modifier": {
      "$ref": "../split-schema/float.json"
    },
    "upper_damage_modifier": {
      "$ref": "../split-schema/float.json"
    }
  },
  "required": [
    "type",
    "damage"
  ]
}