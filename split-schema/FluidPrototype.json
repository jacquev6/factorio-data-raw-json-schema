{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/prototypes/FluidPrototype.html",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "fluid"
    },
    "name": {
      "$ref": "../split-schema/string.json"
    },
    "order": {
      "$ref": "../split-schema/Order.json"
    },
    "localised_name": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "localised_description": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "factoriopedia_description": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "subgroup": {
      "$ref": "../split-schema/ItemSubGroupID.json"
    },
    "hidden": {
      "$ref": "../split-schema/bool.json"
    },
    "hidden_in_factoriopedia": {
      "$ref": "../split-schema/bool.json"
    },
    "parameter": {
      "$ref": "../split-schema/bool.json"
    },
    "factoriopedia_simulation": {
      "$ref": "../split-schema/SimulationDefinition.json"
    },
    "factoriopedia_alternative": {
      "$ref": "../split-schema/string.json"
    },
    "icons": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/IconData.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "icon": {
      "$ref": "../split-schema/FileName.json"
    },
    "icon_size": {
      "$ref": "../split-schema/SpriteSizeType.json"
    },
    "default_temperature": {
      "$ref": "../split-schema/float.json"
    },
    "base_color": {
      "$ref": "../split-schema/Color.json"
    },
    "flow_color": {
      "$ref": "../split-schema/Color.json"
    },
    "visualization_color": {
      "$ref": "../split-schema/Color.json"
    },
    "max_temperature": {
      "$ref": "../split-schema/float.json"
    },
    "heat_capacity": {
      "$ref": "../split-schema/Energy.json"
    },
    "fuel_value": {
      "$ref": "../split-schema/Energy.json"
    },
    "emissions_multiplier": {
      "$ref": "../split-schema/double.json"
    },
    "gas_temperature": {
      "$ref": "../split-schema/float.json"
    },
    "auto_barrel": {
      "$ref": "../split-schema/bool.json"
    }
  },
  "additionalProperties": false,
  "required": [
    "base_color",
    "default_temperature",
    "flow_color",
    "name",
    "type"
  ]
}