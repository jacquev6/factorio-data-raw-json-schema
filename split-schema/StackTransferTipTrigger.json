{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/StackTransferTipTrigger.html",
  "type": "object",
  "properties": {
    "count": {
      "$ref": "../split-schema/uint32.json"
    },
    "type": {
      "type": "string",
      "const": "stack-transfer"
    },
    "transfer": {
      "anyOf": [
        {
          "type": "string",
          "const": "stack"
        },
        {
          "type": "string",
          "const": "inventory"
        },
        {
          "type": "string",
          "const": "whole-inventory"
        }
      ]
    }
  },
  "required": [
    "type"
  ]
}