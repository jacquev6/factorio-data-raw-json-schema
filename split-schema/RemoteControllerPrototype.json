{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/prototypes/RemoteControllerPrototype.html",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "remote-controller"
    },
    "name": {
      "$ref": "../split-schema/string.json"
    },
    "movement_speed": {
      "$ref": "../split-schema/double.json"
    }
  },
  "additionalProperties": false,
  "required": [
    "type",
    "name",
    "movement_speed"
  ]
}