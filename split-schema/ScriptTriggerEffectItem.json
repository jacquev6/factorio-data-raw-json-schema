{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/ScriptTriggerEffectItem.html",
  "type": "object",
  "properties": {
    "repeat_count": {
      "$ref": "../split-schema/double.json"
    },
    "repeat_count_deviation": {
      "$ref": "../split-schema/uint16.json"
    },
    "probability": {
      "$ref": "../split-schema/float.json"
    },
    "affects_target": {
      "$ref": "../split-schema/bool.json"
    },
    "show_in_tooltip": {
      "$ref": "../split-schema/bool.json"
    },
    "damage_type_filters": {
      "$ref": "../split-schema/DamageTypeFilters.json"
    },
    "type": {
      "type": "string",
      "const": "script"
    },
    "effect_id": {
      "$ref": "../split-schema/string.json"
    }
  },
  "required": [
    "type",
    "effect_id"
  ]
}