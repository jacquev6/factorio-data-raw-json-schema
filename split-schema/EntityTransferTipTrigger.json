{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/EntityTransferTipTrigger.html",
  "type": "object",
  "properties": {
    "count": {
      "$ref": "../split-schema/uint32.json"
    },
    "type": {
      "type": "string",
      "const": "entity-transfer"
    },
    "transfer": {
      "anyOf": [
        {
          "type": "string",
          "const": "in"
        },
        {
          "type": "string",
          "const": "out"
        }
      ]
    }
  },
  "required": [
    "type"
  ]
}