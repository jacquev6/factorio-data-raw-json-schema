{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/SpriteParameters.html",
  "allOf": [
    {
      "$ref": "../split-schema/SpriteSource.json"
    },
    {
      "type": "object",
      "properties": {
        "priority": {
          "$ref": "../split-schema/SpritePriority.json"
        },
        "flags": {
          "$ref": "../split-schema/SpriteFlags.json"
        },
        "shift": {
          "$ref": "../split-schema/Vector.json"
        },
        "rotate_shift": {
          "$ref": "../split-schema/bool.json"
        },
        "apply_special_effect": {
          "$ref": "../split-schema/bool.json"
        },
        "scale": {
          "$ref": "../split-schema/double.json"
        },
        "draw_as_shadow": {
          "$ref": "../split-schema/bool.json"
        },
        "draw_as_glow": {
          "$ref": "../split-schema/bool.json"
        },
        "draw_as_light": {
          "$ref": "../split-schema/bool.json"
        },
        "mipmap_count": {
          "$ref": "../split-schema/uint8.json"
        },
        "apply_runtime_tint": {
          "$ref": "../split-schema/bool.json"
        },
        "tint_as_overlay": {
          "$ref": "../split-schema/bool.json"
        },
        "invert_colors": {
          "$ref": "../split-schema/bool.json"
        },
        "tint": {
          "$ref": "../split-schema/Color.json"
        },
        "blend_mode": {
          "$ref": "../split-schema/BlendMode.json"
        },
        "generate_sdf": {
          "$ref": "../split-schema/bool.json"
        },
        "surface": {
          "$ref": "../split-schema/SpriteUsageSurfaceHint.json"
        },
        "usage": {
          "$ref": "../split-schema/SpriteUsageHint.json"
        }
      }
    }
  ]
}