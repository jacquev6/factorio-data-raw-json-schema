{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/prototypes/ChainActiveTriggerPrototype.html",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "chain-active-trigger"
    },
    "name": {
      "$ref": "../split-schema/string.json"
    },
    "order": {
      "$ref": "../split-schema/Order.json"
    },
    "localised_name": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "localised_description": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "factoriopedia_description": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "subgroup": {
      "$ref": "../split-schema/ItemSubGroupID.json"
    },
    "hidden": {
      "$ref": "../split-schema/bool.json"
    },
    "hidden_in_factoriopedia": {
      "$ref": "../split-schema/bool.json"
    },
    "parameter": {
      "$ref": "../split-schema/bool.json"
    },
    "factoriopedia_simulation": {
      "$ref": "../split-schema/SimulationDefinition.json"
    },
    "factoriopedia_alternative": {
      "$ref": "../split-schema/string.json"
    },
    "action": {
      "$ref": "../split-schema/Trigger.json"
    },
    "max_jumps": {
      "$ref": "../split-schema/uint32.json"
    },
    "max_range_per_jump": {
      "$ref": "../split-schema/double.json"
    },
    "max_range": {
      "$ref": "../split-schema/double.json"
    },
    "jump_delay_ticks": {
      "$ref": "../split-schema/MapTick.json"
    },
    "fork_chance": {
      "$ref": "../split-schema/double.json"
    },
    "fork_chance_increase_per_quality_level": {
      "$ref": "../split-schema/double.json"
    },
    "max_forks_per_jump": {
      "$ref": "../split-schema/uint32.json"
    },
    "max_forks": {
      "$ref": "../split-schema/uint32.json"
    }
  },
  "required": [
    "type",
    "name"
  ]
}