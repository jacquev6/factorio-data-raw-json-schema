{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/ProjectileTriggerDelivery.html",
  "type": "object",
  "properties": {
    "source_effects": {
      "$ref": "../split-schema/TriggerEffect.json"
    },
    "target_effects": {
      "$ref": "../split-schema/TriggerEffect.json"
    },
    "type": {
      "type": "string",
      "const": "projectile"
    },
    "projectile": {
      "$ref": "../split-schema/EntityID.json"
    },
    "starting_speed": {
      "$ref": "../split-schema/float.json"
    },
    "starting_speed_deviation": {
      "$ref": "../split-schema/float.json"
    },
    "direction_deviation": {
      "$ref": "../split-schema/float.json"
    },
    "range_deviation": {
      "$ref": "../split-schema/float.json"
    },
    "max_range": {
      "$ref": "../split-schema/double.json"
    },
    "min_range": {
      "$ref": "../split-schema/double.json"
    }
  },
  "required": [
    "type",
    "projectile",
    "starting_speed"
  ]
}