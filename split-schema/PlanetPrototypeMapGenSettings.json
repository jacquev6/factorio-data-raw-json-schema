{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/PlanetPrototypeMapGenSettings.html",
  "type": "object",
  "properties": {
    "cliff_settings": {
      "$ref": "../split-schema/CliffPlacementSettings.json"
    },
    "territory_settings": {
      "$ref": "../split-schema/TerritorySettings.json"
    },
    "autoplace_controls": {
      "type": "object",
      "additionalProperties": {
        "$ref": "../split-schema/FrequencySizeRichness.json"
      },
      "propertyNames": {
        "$ref": "../split-schema/AutoplaceControlID.json"
      }
    },
    "autoplace_settings": {
      "type": "object",
      "additionalProperties": {
        "$ref": "../split-schema/AutoplaceSettings.json"
      },
      "propertyNames": {
        "anyOf": [
          {
            "type": "string",
            "const": "entity"
          },
          {
            "type": "string",
            "const": "tile"
          },
          {
            "type": "string",
            "const": "decorative"
          }
        ]
      }
    },
    "property_expression_names": {
      "type": "object",
      "additionalProperties": {
        "anyOf": [
          {
            "$ref": "../split-schema/string.json"
          },
          {
            "$ref": "../split-schema/bool.json"
          },
          {
            "$ref": "../split-schema/double.json"
          }
        ]
      },
      "propertyNames": {
        "$ref": "../split-schema/string.json"
      }
    },
    "moisture_climate_control": {
      "$ref": "../split-schema/bool.json"
    },
    "aux_climate_control": {
      "$ref": "../split-schema/bool.json"
    }
  }
}