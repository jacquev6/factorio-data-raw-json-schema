{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/TileMainPictures.html",
  "type": "object",
  "properties": {
    "picture": {
      "$ref": "../split-schema/FileName.json"
    },
    "scale": {
      "$ref": "../split-schema/float.json"
    },
    "x": {
      "$ref": "../split-schema/SpriteSizeType.json"
    },
    "y": {
      "$ref": "../split-schema/SpriteSizeType.json"
    },
    "line_length": {
      "$ref": "../split-schema/uint8.json"
    },
    "count": {
      "$ref": "../split-schema/uint8.json"
    },
    "size": {
      "$ref": "../split-schema/uint32.json"
    },
    "probability": {
      "$ref": "../split-schema/double.json"
    },
    "weights": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/double.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    }
  },
  "required": [
    "picture",
    "size"
  ]
}