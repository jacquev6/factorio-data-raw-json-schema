{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/ElectricEnergySource.html",
  "type": "object",
  "properties": {
    "emissions_per_minute": {
      "type": "object",
      "additionalProperties": {
        "$ref": "../split-schema/double.json"
      },
      "propertyNames": {
        "$ref": "../split-schema/AirbornePollutantID.json"
      }
    },
    "render_no_power_icon": {
      "$ref": "../split-schema/bool.json"
    },
    "render_no_network_icon": {
      "$ref": "../split-schema/bool.json"
    },
    "type": {
      "type": "string",
      "const": "electric"
    },
    "buffer_capacity": {
      "$ref": "../split-schema/Energy.json"
    },
    "usage_priority": {
      "$ref": "../split-schema/ElectricUsagePriority.json"
    },
    "input_flow_limit": {
      "$ref": "../split-schema/Energy.json"
    },
    "output_flow_limit": {
      "$ref": "../split-schema/Energy.json"
    },
    "drain": {
      "$ref": "../split-schema/Energy.json"
    }
  },
  "required": [
    "type",
    "usage_priority"
  ]
}