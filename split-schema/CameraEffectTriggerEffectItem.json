{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/CameraEffectTriggerEffectItem.html",
  "type": "object",
  "properties": {
    "repeat_count": {
      "$ref": "../split-schema/double.json"
    },
    "repeat_count_deviation": {
      "$ref": "../split-schema/uint16.json"
    },
    "probability": {
      "$ref": "../split-schema/float.json"
    },
    "affects_target": {
      "$ref": "../split-schema/bool.json"
    },
    "show_in_tooltip": {
      "$ref": "../split-schema/bool.json"
    },
    "damage_type_filters": {
      "$ref": "../split-schema/DamageTypeFilters.json"
    },
    "type": {
      "type": "string",
      "const": "camera-effect"
    },
    "duration": {
      "$ref": "../split-schema/uint8.json"
    },
    "ease_in_duration": {
      "$ref": "../split-schema/uint8.json"
    },
    "ease_out_duration": {
      "$ref": "../split-schema/uint8.json"
    },
    "delay": {
      "$ref": "../split-schema/uint8.json"
    },
    "full_strength_max_distance": {
      "$ref": "../split-schema/uint16.json"
    },
    "max_distance": {
      "$ref": "../split-schema/uint16.json"
    },
    "strength": {
      "$ref": "../split-schema/float.json"
    }
  },
  "required": [
    "type",
    "duration"
  ]
}