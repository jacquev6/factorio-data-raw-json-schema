{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/CircularParticleCreationSpecification.html",
  "type": "object",
  "properties": {
    "name": {
      "$ref": "../split-schema/ParticleID.json"
    },
    "starting_frame_speed": {
      "$ref": "../split-schema/float.json"
    },
    "direction": {
      "$ref": "../split-schema/float.json"
    },
    "direction_deviation": {
      "$ref": "../split-schema/float.json"
    },
    "speed": {
      "$ref": "../split-schema/float.json"
    },
    "speed_deviation": {
      "$ref": "../split-schema/float.json"
    },
    "starting_frame_speed_deviation": {
      "$ref": "../split-schema/float.json"
    },
    "height": {
      "$ref": "../split-schema/float.json"
    },
    "height_deviation": {
      "$ref": "../split-schema/float.json"
    },
    "vertical_speed": {
      "$ref": "../split-schema/float.json"
    },
    "vertical_speed_deviation": {
      "$ref": "../split-schema/float.json"
    },
    "center": {
      "$ref": "../split-schema/Vector.json"
    },
    "creation_distance": {
      "$ref": "../split-schema/double.json"
    },
    "creation_distance_orientation": {
      "$ref": "../split-schema/double.json"
    },
    "use_source_position": {
      "$ref": "../split-schema/bool.json"
    }
  },
  "required": [
    "name",
    "starting_frame_speed"
  ]
}