{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/prototypes/DontBuildEntityAchievementPrototype.html",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "dont-build-entity-achievement"
    },
    "name": {
      "$ref": "../split-schema/string.json"
    },
    "order": {
      "$ref": "../split-schema/Order.json"
    },
    "localised_name": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "localised_description": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "factoriopedia_description": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "subgroup": {
      "$ref": "../split-schema/ItemSubGroupID.json"
    },
    "hidden": {
      "$ref": "../split-schema/bool.json"
    },
    "hidden_in_factoriopedia": {
      "$ref": "../split-schema/bool.json"
    },
    "parameter": {
      "$ref": "../split-schema/bool.json"
    },
    "factoriopedia_simulation": {
      "$ref": "../split-schema/SimulationDefinition.json"
    },
    "factoriopedia_alternative": {
      "$ref": "../split-schema/string.json"
    },
    "icons": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/IconData.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "icon": {
      "$ref": "../split-schema/FileName.json"
    },
    "icon_size": {
      "$ref": "../split-schema/SpriteSizeType.json"
    },
    "steam_stats_name": {
      "$ref": "../split-schema/string.json"
    },
    "allowed_without_fight": {
      "$ref": "../split-schema/bool.json"
    },
    "objective_condition": {
      "anyOf": [
        {
          "type": "string",
          "const": "game-finished"
        },
        {
          "type": "string",
          "const": "rocket-launched"
        },
        {
          "type": "string",
          "const": "late-research"
        }
      ]
    },
    "dont_build": {
      "anyOf": [
        {
          "$ref": "../split-schema/EntityID.json"
        },
        {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "../split-schema/EntityID.json"
              }
            },
            {
              "type": "object",
              "additionalProperties": false
            }
          ]
        }
      ]
    },
    "amount": {
      "$ref": "../split-schema/uint32.json"
    },
    "research_with": {
      "anyOf": [
        {
          "$ref": "../split-schema/ItemID.json"
        },
        {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "../split-schema/ItemID.json"
              }
            },
            {
              "type": "object",
              "additionalProperties": false
            }
          ]
        }
      ]
    }
  },
  "required": [
    "type",
    "name",
    "dont_build"
  ]
}