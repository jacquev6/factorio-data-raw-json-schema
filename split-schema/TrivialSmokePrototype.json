    },
    "fade_away_duration": {
      "$ref": "../split-schema/uint32.json"
    },
    "fade_in_duration": {
      "$ref": "../split-schema/uint32.json"
    },
    "glow_fade_away_duration": {
      "$ref": "../split-schema/uint32.json"
