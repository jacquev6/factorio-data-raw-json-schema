{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/SwitchStyleSpecification.html",
  "type": "object",
  "properties": {
    "parent": {
      "$ref": "../split-schema/string.json"
    },
    "horizontal_align": {
      "$ref": "../split-schema/HorizontalAlign.json"
