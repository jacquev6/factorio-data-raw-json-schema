      ]
    },
    "size": {
      "anyOf": [
        {
          "$ref": "../split-schema/uint32.json"
        },
        {
          "type": "array",
          "items": [
            {
              "$ref": "../split-schema/uint32.json"
            },
            {
              "$ref": "../split-schema/uint32.json"
            }
          ],
          "minItems": 2,
          "maxItems": 2
        }
      ]
