    },
    "top_padding": {
      "$ref": "../split-schema/int16.json"
    },
    "right_padding": {
      "$ref": "../split-schema/int16.json"
    },
    "bottom_padding": {
      "$ref": "../split-schema/int16.json"
    },
    "left_padding": {
      "$ref": "../split-schema/int16.json"
    },
    "margin": {
      "$ref": "../split-schema/int16.json"
    },
    "top_margin": {
      "$ref": "../split-schema/int16.json"
    },
    "right_margin": {
      "$ref": "../split-schema/int16.json"
    },
    "bottom_margin": {
      "$ref": "../split-schema/int16.json"
    },
    "left_margin": {
      "$ref": "../split-schema/int16.json"
