{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/KillTipTrigger.html",
  "type": "object",
  "properties": {
    "count": {
      "$ref": "../split-schema/uint32.json"
    },
    "type": {
      "type": "string",
      "const": "kill"
    },
    "entity": {
      "$ref": "../split-schema/EntityID.json"
    },
    "match_type_only": {
      "$ref": "../split-schema/bool.json"
    },
    "damage_type": {
      "$ref": "../split-schema/DamageTypeID.json"
    }
  },
  "required": [
    "type"
  ]
}