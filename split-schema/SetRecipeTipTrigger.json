{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/SetRecipeTipTrigger.html",
  "allOf": [
    {
      "$ref": "../split-schema/CountBasedTipTrigger.json"
    },
    {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "const": "set-recipe"
        },
        "recipe": {
          "$ref": "../split-schema/RecipeID.json"
        },
        "machine": {
          "$ref": "../split-schema/EntityID.json"
        },
        "consecutive": {
          "$ref": "../split-schema/bool.json"
        },
        "any_quality": {
          "$ref": "../split-schema/bool.json"
        },
        "uses_fluid": {
          "$ref": "../split-schema/bool.json"
        }
      },
      "required": [
        "type"
      ]
    }
  ]
}