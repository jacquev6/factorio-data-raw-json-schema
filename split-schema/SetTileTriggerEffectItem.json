{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/SetTileTriggerEffectItem.html",
  "type": "object",
  "properties": {
    "repeat_count": {
      "$ref": "../split-schema/double.json"
    },
    "repeat_count_deviation": {
      "$ref": "../split-schema/uint16.json"
    },
    "probability": {
      "$ref": "../split-schema/float.json"
    },
    "affects_target": {
      "$ref": "../split-schema/bool.json"
    },
    "show_in_tooltip": {
      "$ref": "../split-schema/bool.json"
    },
    "damage_type_filters": {
      "$ref": "../split-schema/DamageTypeFilters.json"
    },
    "type": {
      "type": "string",
      "const": "set-tile"
    },
    "tile_name": {
      "$ref": "../split-schema/TileID.json"
    },
    "radius": {
      "$ref": "../split-schema/float.json"
    },
    "apply_projection": {
      "$ref": "../split-schema/bool.json"
    },
    "apply_on_space_platform": {
      "$ref": "../split-schema/bool.json"
    },
    "tile_collision_mask": {
      "$ref": "../split-schema/CollisionMaskConnector.json"
    }
  },
  "required": [
    "type",
    "tile_name",
    "radius"
  ]
}