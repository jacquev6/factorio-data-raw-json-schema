{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/prototypes/SoundPrototype.html",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "sound"
    },
    "name": {
      "$ref": "../split-schema/string.json"
    },
    "category": {
      "$ref": "../split-schema/SoundType.json"
    },
    "priority": {
      "$ref": "../split-schema/uint8.json"
    },
    "aggregation": {
      "$ref": "../split-schema/AggregationSpecification.json"
    },
    "allow_random_repeat": {
      "$ref": "../split-schema/bool.json"
    },
    "audible_distance_modifier": {
      "$ref": "../split-schema/double.json"
    },
    "game_controller_vibration_data": {
      "$ref": "../split-schema/GameControllerVibrationData.json"
    },
    "advanced_volume_control": {
      "$ref": "../split-schema/AdvancedVolumeControl.json"
    },
    "speed_smoothing_window_size": {
      "$ref": "../split-schema/uint32.json"
    },
    "variations": {
      "anyOf": [
        {
          "$ref": "../split-schema/SoundDefinition.json"
        },
        {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "../split-schema/SoundDefinition.json"
              }
            },
            {
              "type": "object",
              "additionalProperties": false
            }
          ]
        }
      ]
    },
    "filename": {
      "$ref": "../split-schema/FileName.json"
    },
    "volume": {
      "$ref": "../split-schema/float.json"
    },
    "min_volume": {
      "$ref": "../split-schema/float.json"
    },
    "max_volume": {
      "$ref": "../split-schema/float.json"
    },
    "preload": {
      "$ref": "../split-schema/bool.json"
    },
    "speed": {
      "$ref": "../split-schema/float.json"
    },
    "min_speed": {
      "$ref": "../split-schema/float.json"
    },
    "max_speed": {
      "$ref": "../split-schema/float.json"
    },
    "modifiers": {
      "anyOf": [
        {
          "$ref": "../split-schema/SoundModifier.json"
        },
        {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "../split-schema/SoundModifier.json"
              }
            },
            {
              "type": "object",
              "additionalProperties": false
            }
          ]
        }
      ]
    }
  },
  "required": [
    "type",
    "name"
  ]
}