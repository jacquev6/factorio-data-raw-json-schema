{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/prototypes/AmbientSound.html",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "ambient-sound"
    },
    "name": {
      "$ref": "../split-schema/string.json"
    },
    "weight": {
      "$ref": "../split-schema/double.json"
    },
    "track_type": {
      "$ref": "../split-schema/AmbientSoundType.json"
    },
    "planet": {
      "$ref": "../split-schema/SpaceLocationID.json"
    },
    "sound": {
      "$ref": "../split-schema/Sound.json"
    },
    "variable_sound": {
      "$ref": "../split-schema/VariableAmbientSoundVariableSound.json"
    }
  },
  "additionalProperties": false,
  "required": [
    "name",
    "track_type",
    "type"
  ]
}