{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/SimulationDefinition.html",
  "type": "object",
  "properties": {
    "planet": {
      "$ref": "../split-schema/SpaceLocationID.json"
    },
    "game_view_settings": {
      "$ref": "../split-schema/GameViewSettings.json"
    },
    "save": {
      "$ref": "../split-schema/FileName.json"
    },
    "init_file": {
      "$ref": "../split-schema/FileName.json"
    },
    "init": {
      "$ref": "../split-schema/string.json"
    },
    "update_file": {
      "$ref": "../split-schema/FileName.json"
    },
    "update": {
      "$ref": "../split-schema/string.json"
    },
    "mods": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/string.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "init_update_count": {
      "$ref": "../split-schema/uint32.json"
    },
    "length": {
      "$ref": "../split-schema/uint32.json"
    },
    "generate_map": {
      "$ref": "../split-schema/bool.json"
    },
    "checkboard": {
      "$ref": "../split-schema/bool.json"
    },
    "hide_health_bars": {
      "$ref": "../split-schema/bool.json"
    },
    "mute_technology_finished_sound": {
      "$ref": "../split-schema/bool.json"
    },
    "mute_alert_sounds": {
      "$ref": "../split-schema/bool.json"
    },
    "volume_modifier": {
      "$ref": "../split-schema/float.json"
    },
    "override_volume": {
      "$ref": "../split-schema/bool.json"
    },
    "mute_wind_sounds": {
      "$ref": "../split-schema/bool.json"
    },
    "hide_factoriopedia_gradient": {
      "$ref": "../split-schema/bool.json"
    }
  }
}