{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/prototypes/QualityPrototype.html",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "quality"
    },
    "name": {
      "$ref": "../split-schema/string.json"
    },
    "order": {
      "$ref": "../split-schema/Order.json"
    },
    "localised_name": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "localised_description": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "factoriopedia_description": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "subgroup": {
      "$ref": "../split-schema/ItemSubGroupID.json"
    },
    "hidden": {
      "$ref": "../split-schema/bool.json"
    },
    "hidden_in_factoriopedia": {
      "$ref": "../split-schema/bool.json"
    },
    "parameter": {
      "$ref": "../split-schema/bool.json"
    },
    "factoriopedia_simulation": {
      "$ref": "../split-schema/SimulationDefinition.json"
    },
    "factoriopedia_alternative": {
      "$ref": "../split-schema/string.json"
    },
    "draw_sprite_by_default": {
      "$ref": "../split-schema/bool.json"
    },
    "color": {
      "$ref": "../split-schema/Color.json"
    },
    "level": {
      "$ref": "../split-schema/uint32.json"
    },
    "next": {
      "$ref": "../split-schema/QualityID.json"
    },
    "next_probability": {
      "$ref": "../split-schema/double.json"
    },
    "icons": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/IconData.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "icon": {
      "$ref": "../split-schema/FileName.json"
    },
    "icon_size": {
      "$ref": "../split-schema/SpriteSizeType.json"
    },
    "beacon_power_usage_multiplier": {
      "$ref": "../split-schema/float.json"
    },
    "mining_drill_resource_drain_multiplier": {
      "$ref": "../split-schema/float.json"
    },
    "science_pack_drain_multiplier": {
      "$ref": "../split-schema/float.json"
    }
  },
  "additionalProperties": false,
  "required": [
    "type",
    "name",
    "color",
    "level"
  ]
}