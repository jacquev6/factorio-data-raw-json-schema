{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/prototypes/GuiStyle.html",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "gui-style"
    },
    "name": {
      "$ref": "../split-schema/string.json"
    },
    "order": {
      "$ref": "../split-schema/Order.json"
    },
    "localised_name": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "localised_description": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "factoriopedia_description": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "subgroup": {
      "$ref": "../split-schema/ItemSubGroupID.json"
    },
    "hidden": {
      "$ref": "../split-schema/bool.json"
    },
    "hidden_in_factoriopedia": {
      "$ref": "../split-schema/bool.json"
    },
    "parameter": {
      "$ref": "../split-schema/bool.json"
    },
    "factoriopedia_simulation": {
      "$ref": "../split-schema/SimulationDefinition.json"
    },
    "default_tileset": {
      "$ref": "../split-schema/FileName.json"
    },
    "default_sprite_scale": {
      "$ref": "../split-schema/double.json"
    },
    "default_sprite_priority": {
      "$ref": "../split-schema/SpritePriority.json"
    }
  },
  "additionalProperties": {
    "$ref": "../split-schema/StyleSpecification.json"
  },
  "required": [
    "name",
    "type"
  ]
}