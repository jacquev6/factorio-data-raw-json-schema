{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/prototypes/RecipePrototype.html",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "recipe"
    },
    "name": {
      "$ref": "../split-schema/string.json"
    },
    "order": {
      "$ref": "../split-schema/Order.json"
    },
    "localised_name": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "localised_description": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "factoriopedia_description": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "subgroup": {
      "$ref": "../split-schema/ItemSubGroupID.json"
    },
    "hidden": {
      "$ref": "../split-schema/bool.json"
    },
    "hidden_in_factoriopedia": {
      "$ref": "../split-schema/bool.json"
    },
    "parameter": {
      "$ref": "../split-schema/bool.json"
    },
    "factoriopedia_simulation": {
      "$ref": "../split-schema/SimulationDefinition.json"
    },
    "factoriopedia_alternative": {
      "$ref": "../split-schema/string.json"
    },
    "category": {
      "$ref": "../split-schema/RecipeCategoryID.json"
    },
    "crafting_machine_tint": {
      "$ref": "../split-schema/RecipeTints.json"
    },
    "icons": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/IconData.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "icon": {
      "$ref": "../split-schema/FileName.json"
    },
    "icon_size": {
      "$ref": "../split-schema/SpriteSizeType.json"
    },
    "ingredients": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/IngredientPrototype.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "results": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/ProductPrototype.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "main_product": {
      "$ref": "../split-schema/string.json"
    },
    "energy_required": {
      "$ref": "../split-schema/double.json"
    },
    "emissions_multiplier": {
      "$ref": "../split-schema/double.json"
    },
    "maximum_productivity": {
      "$ref": "../split-schema/double.json"
    },
    "requester_paste_multiplier": {
      "$ref": "../split-schema/uint32.json"
    },
    "overload_multiplier": {
      "$ref": "../split-schema/uint32.json"
    },
    "allow_inserter_overload": {
      "$ref": "../split-schema/bool.json"
    },
    "enabled": {
      "$ref": "../split-schema/bool.json"
    },
    "hide_from_stats": {
      "$ref": "../split-schema/bool.json"
    },
    "hide_from_player_crafting": {
      "$ref": "../split-schema/bool.json"
    },
    "allow_decomposition": {
      "$ref": "../split-schema/bool.json"
    },
    "allow_as_intermediate": {
      "$ref": "../split-schema/bool.json"
    },
    "allow_intermediates": {
      "$ref": "../split-schema/bool.json"
    },
    "always_show_made_in": {
      "$ref": "../split-schema/bool.json"
    },
    "show_amount_in_title": {
      "$ref": "../split-schema/bool.json"
    },
    "always_show_products": {
      "$ref": "../split-schema/bool.json"
    },
    "unlock_results": {
      "$ref": "../split-schema/bool.json"
    },
    "preserve_products_in_machine_output": {
      "$ref": "../split-schema/bool.json"
    },
    "result_is_always_fresh": {
      "$ref": "../split-schema/bool.json"
    },
    "allow_consumption_message": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "allow_speed_message": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "allow_productivity_message": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "allow_pollution_message": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "allow_quality_message": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "surface_conditions": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/SurfaceCondition.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "hide_from_signal_gui": {
      "$ref": "../split-schema/bool.json"
    },
    "allow_consumption": {
      "$ref": "../split-schema/bool.json"
    },
    "allow_speed": {
      "$ref": "../split-schema/bool.json"
    },
    "allow_productivity": {
      "$ref": "../split-schema/bool.json"
    },
    "allow_pollution": {
      "$ref": "../split-schema/bool.json"
    },
    "allow_quality": {
      "$ref": "../split-schema/bool.json"
    },
    "allowed_module_categories": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/ModuleCategoryID.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "alternative_unlock_methods": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/TechnologyID.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "type",
    "name"
  ]
}