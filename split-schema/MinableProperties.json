{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/MinableProperties.html",
  "type": "object",
  "properties": {
    "mining_time": {
      "$ref": "../split-schema/double.json"
    },
    "include_in_show_counts": {
      "$ref": "../split-schema/bool.json"
    },
    "results": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/ProductPrototype.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "result": {
      "$ref": "../split-schema/ItemID.json"
    },
    "fluid_amount": {
      "$ref": "../split-schema/FluidAmount.json"
    },
    "mining_particle": {
      "$ref": "../split-schema/ParticleID.json"
    },
    "required_fluid": {
      "$ref": "../split-schema/FluidID.json"
    },
    "count": {
      "$ref": "../split-schema/uint16.json"
    },
    "mining_trigger": {
      "$ref": "../split-schema/Trigger.json"
    }
  },
  "required": [
    "mining_time"
  ]
}