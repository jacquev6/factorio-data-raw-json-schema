{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/FluidIngredientPrototype.html",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "fluid"
    },
    "name": {
      "$ref": "../split-schema/FluidID.json"
    },
    "amount": {
      "$ref": "../split-schema/FluidAmount.json"
    },
    "temperature": {
      "$ref": "../split-schema/float.json"
    },
    "minimum_temperature": {
      "$ref": "../split-schema/float.json"
    },
    "maximum_temperature": {
      "$ref": "../split-schema/float.json"
    },
    "ignored_by_stats": {
      "$ref": "../split-schema/FluidAmount.json"
    },
    "fluidbox_index": {
      "$ref": "../split-schema/uint32.json"
    },
    "fluidbox_multiplier": {
      "$ref": "../split-schema/uint8.json"
    }
  },
  "required": [
    "type",
    "name",
    "amount"
  ]
}