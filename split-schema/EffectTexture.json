{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/EffectTexture.html",
  "type": "object",
  "properties": {
    "filename": {
      "$ref": "../split-schema/FileName.json"
    },
    "size": {
      "anyOf": [
        {
          "$ref": "../split-schema/SpriteSizeType.json"
        },
        {
          "type": "array",
          "items": [
            {
              "$ref": "../split-schema/SpriteSizeType.json"
            },
            {
              "$ref": "../split-schema/SpriteSizeType.json"
            }
          ],
          "minItems": 2,
          "maxItems": 2
        }
      ]
    },
    "width": {
      "$ref": "../split-schema/SpriteSizeType.json"
    },
    "height": {
      "$ref": "../split-schema/SpriteSizeType.json"
    },
    "x": {
      "$ref": "../split-schema/SpriteSizeType.json"
    },
    "y": {
      "$ref": "../split-schema/SpriteSizeType.json"
    },
    "position": {
      "type": "array",
      "items": [
        {
          "$ref": "../split-schema/SpriteSizeType.json"
        },
        {
          "$ref": "../split-schema/SpriteSizeType.json"
        }
      ],
      "minItems": 2,
      "maxItems": 2
    },
    "load_in_minimal_mode": {
      "$ref": "../split-schema/bool.json"
    },
    "premul_alpha": {
      "$ref": "../split-schema/bool.json"
    },
    "allow_forced_downscale": {
      "$ref": "../split-schema/bool.json"
    }
  }
}