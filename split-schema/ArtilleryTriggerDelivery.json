{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/ArtilleryTriggerDelivery.html",
  "allOf": [
    {
      "$ref": "../split-schema/TriggerDeliveryItem.json"
    },
    {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "const": "artillery"
        },
        "projectile": {
          "$ref": "../split-schema/EntityID.json"
        },
        "starting_speed": {
          "$ref": "../split-schema/float.json"
        },
        "starting_speed_deviation": {
          "$ref": "../split-schema/float.json"
        },
        "direction_deviation": {
          "$ref": "../split-schema/float.json"
        },
        "range_deviation": {
          "$ref": "../split-schema/float.json"
        },
        "trigger_fired_artillery": {
          "$ref": "../split-schema/bool.json"
        }
      },
      "required": [
        "type",
        "projectile",
        "starting_speed"
      ]
    }
  ]
}