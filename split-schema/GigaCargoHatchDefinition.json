{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/GigaCargoHatchDefinition.html",
  "type": "object",
  "properties": {
    "hatch_graphics_back": {
      "$ref": "../split-schema/Animation.json"
    },
    "hatch_graphics_front": {
      "$ref": "../split-schema/Animation.json"
    },
    "hatch_render_layer_back": {
      "$ref": "../split-schema/RenderLayer.json"
    },
    "hatch_render_layer_front": {
      "$ref": "../split-schema/RenderLayer.json"
    },
    "covered_hatches": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/uint32.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "opening_sound": {
      "$ref": "../split-schema/InterruptibleSound.json"
    },
    "closing_sound": {
      "$ref": "../split-schema/InterruptibleSound.json"
    }
  },
  "required": [
    "covered_hatches"
  ]
}