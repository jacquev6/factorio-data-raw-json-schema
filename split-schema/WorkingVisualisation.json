{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/WorkingVisualisation.html",
  "type": "object",
  "properties": {
    "render_layer": {
      "$ref": "../split-schema/RenderLayer.json"
    },
    "fadeout": {
      "$ref": "../split-schema/bool.json"
    },
    "synced_fadeout": {
      "$ref": "../split-schema/bool.json"
    },
    "constant_speed": {
      "$ref": "../split-schema/bool.json"
    },
    "always_draw": {
      "$ref": "../split-schema/bool.json"
    },
    "animated_shift": {
      "$ref": "../split-schema/bool.json"
    },
    "align_to_waypoint": {
      "$ref": "../split-schema/bool.json"
    },
    "mining_drill_scorch_mark": {
      "$ref": "../split-schema/bool.json"
    },
    "secondary_draw_order": {
      "$ref": "../split-schema/int8.json"
    },
    "light": {
      "$ref": "../split-schema/LightDefinition.json"
    },
    "effect": {
      "anyOf": [
        {
          "type": "string",
          "const": "flicker"
        },
        {
          "type": "string",
          "const": "uranium-glow"
        },
        {
          "type": "string",
          "const": "none"
        }
      ]
    },
    "apply_recipe_tint": {
      "anyOf": [
        {
          "type": "string",
          "const": "primary"
        },
        {
          "type": "string",
          "const": "secondary"
        },
        {
          "type": "string",
          "const": "tertiary"
        },
        {
          "type": "string",
          "const": "quaternary"
        },
        {
          "type": "string",
          "const": "none"
        }
      ]
    },
    "apply_tint": {
      "anyOf": [
        {
          "type": "string",
          "const": "resource-color"
        },
        {
          "type": "string",
          "const": "input-fluid-base-color"
        },
        {
          "type": "string",
          "const": "input-fluid-flow-color"
        },
        {
          "type": "string",
          "const": "status"
        },
        {
          "type": "string",
          "const": "none"
        },
        {
          "type": "string",
          "const": "visual-state-color"
        }
      ]
    },
    "north_animation": {
      "$ref": "../split-schema/Animation.json"
    },
    "east_animation": {
      "$ref": "../split-schema/Animation.json"
    },
    "south_animation": {
      "$ref": "../split-schema/Animation.json"
    },
    "west_animation": {
      "$ref": "../split-schema/Animation.json"
    },
    "north_position": {
      "$ref": "../split-schema/Vector.json"
    },
    "east_position": {
      "$ref": "../split-schema/Vector.json"
    },
    "south_position": {
      "$ref": "../split-schema/Vector.json"
    },
    "west_position": {
      "$ref": "../split-schema/Vector.json"
    },
    "north_secondary_draw_order": {
      "$ref": "../split-schema/int8.json"
    },
    "east_secondary_draw_order": {
      "$ref": "../split-schema/int8.json"
    },
    "south_secondary_draw_order": {
      "$ref": "../split-schema/int8.json"
    },
    "west_secondary_draw_order": {
      "$ref": "../split-schema/int8.json"
    },
    "north_fog_mask": {
      "$ref": "../split-schema/FogMaskShapeDefinition.json"
    },
    "east_fog_mask": {
      "$ref": "../split-schema/FogMaskShapeDefinition.json"
    },
    "south_fog_mask": {
      "$ref": "../split-schema/FogMaskShapeDefinition.json"
    },
    "west_fog_mask": {
      "$ref": "../split-schema/FogMaskShapeDefinition.json"
    },
    "fog_mask": {
      "$ref": "../split-schema/FogMaskShapeDefinition.json"
    },
    "animation": {
      "$ref": "../split-schema/Animation.json"
    },
    "draw_in_states": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/string.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "draw_when_state_filter_matches": {
      "$ref": "../split-schema/bool.json"
    },
    "enabled_by_name": {
      "$ref": "../split-schema/bool.json"
    },
    "name": {
      "$ref": "../split-schema/string.json"
    },
    "enabled_in_animated_shift_during_waypoint_stop": {
      "$ref": "../split-schema/bool.json"
    },
    "enabled_in_animated_shift_during_transition": {
      "$ref": "../split-schema/bool.json"
    },
    "frame_based_on_shift_animation_progress": {
      "$ref": "../split-schema/bool.json"
    },
    "scorch_mark_fade_out_duration": {
      "$ref": "../split-schema/uint16.json"
    },
    "scorch_mark_lifetime": {
      "$ref": "../split-schema/uint16.json"
    },
    "scorch_mark_fade_in_frames": {
      "$ref": "../split-schema/uint8.json"
    }
  }
}