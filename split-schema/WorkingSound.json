{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/WorkingSound.html",
  "anyOf": [
    {
      "type": "object",
      "properties": {
        "sound": {
          "$ref": "../split-schema/Sound.json"
        },
        "audible_distance_modifier": {
          "$ref": "../split-schema/double.json"
        },
        "probability": {
          "$ref": "../split-schema/double.json"
        },
        "fade_in_ticks": {
          "$ref": "../split-schema/uint32.json"
        },
        "fade_out_ticks": {
          "$ref": "../split-schema/uint32.json"
        },
        "activity_to_volume_modifiers": {
          "$ref": "../split-schema/ActivityMatchingModifiers.json"
        },
        "activity_to_speed_modifiers": {
          "$ref": "../split-schema/ActivityMatchingModifiers.json"
        },
        "match_progress_to_activity": {
          "$ref": "../split-schema/bool.json"
        },
        "match_volume_to_activity": {
          "$ref": "../split-schema/bool.json"
        },
        "match_speed_to_activity": {
          "$ref": "../split-schema/bool.json"
        },
        "play_for_working_visualisations": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "../split-schema/string.json"
              }
            },
            {
              "type": "object",
              "additionalProperties": false
            }
          ]
        },
        "volume_smoothing_window_size": {
          "$ref": "../split-schema/uint32.json"
        },
        "main_sounds": {
          "anyOf": [
            {
              "$ref": "../split-schema/MainSound.json"
            },
            {
              "oneOf": [
                {
                  "type": "array",
                  "items": {
                    "$ref": "../split-schema/MainSound.json"
                  }
                },
                {
                  "type": "object",
                  "additionalProperties": false
                }
              ]
            }
          ]
        },
        "apparent_volume": {
          "$ref": "../split-schema/float.json"
        },
        "max_sounds_per_type": {
          "$ref": "../split-schema/uint8.json"
        },
        "extra_sounds_ignore_limit": {
          "$ref": "../split-schema/bool.json"
        },
        "persistent": {
          "$ref": "../split-schema/bool.json"
        },
        "use_doppler_shift": {
          "$ref": "../split-schema/bool.json"
        },
        "idle_sound": {
          "$ref": "../split-schema/Sound.json"
        },
        "activate_sound": {
          "$ref": "../split-schema/Sound.json"
        },
        "deactivate_sound": {
          "$ref": "../split-schema/Sound.json"
        },
        "sound_accents": {
          "anyOf": [
            {
              "$ref": "../split-schema/SoundAccent.json"
            },
            {
              "oneOf": [
                {
                  "type": "array",
                  "items": {
                    "$ref": "../split-schema/SoundAccent.json"
                  }
                },
                {
                  "type": "object",
                  "additionalProperties": false
                }
              ]
            }
          ]
        }
      }
    },
    {
      "$ref": "../split-schema/Sound.json"
    }
  ]
}