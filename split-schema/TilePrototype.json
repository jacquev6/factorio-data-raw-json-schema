{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/prototypes/TilePrototype.html",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "tile"
    },
    "name": {
      "$ref": "../split-schema/string.json"
    },
    "order": {
      "$ref": "../split-schema/Order.json"
    },
    "localised_name": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "localised_description": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "factoriopedia_description": {
      "$ref": "../split-schema/LocalisedString.json"
    },
    "subgroup": {
      "$ref": "../split-schema/ItemSubGroupID.json"
    },
    "hidden": {
      "$ref": "../split-schema/bool.json"
    },
    "hidden_in_factoriopedia": {
      "$ref": "../split-schema/bool.json"
    },
    "parameter": {
      "$ref": "../split-schema/bool.json"
    },
    "factoriopedia_simulation": {
      "$ref": "../split-schema/SimulationDefinition.json"
    },
    "factoriopedia_alternative": {
      "$ref": "../split-schema/string.json"
    },
    "collision_mask": {
      "$ref": "../split-schema/CollisionMaskConnector.json"
    },
    "layer": {
      "$ref": "../split-schema/uint8.json"
    },
    "build_animations": {
      "$ref": "../split-schema/Animation4Way.json"
    },
    "build_animations_background": {
      "$ref": "../split-schema/Animation4Way.json"
    },
    "built_animation_frame": {
      "$ref": "../split-schema/uint32.json"
    },
    "variants": {
      "$ref": "../split-schema/TileTransitionsVariants.json"
    },
    "map_color": {
      "$ref": "../split-schema/Color.json"
    },
    "icons": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/IconData.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "icon": {
      "$ref": "../split-schema/FileName.json"
    },
    "icon_size": {
      "$ref": "../split-schema/SpriteSizeType.json"
    },
    "lowland_fog": {
      "$ref": "../split-schema/bool.json"
    },
    "transition_overlay_layer_offset": {
      "$ref": "../split-schema/int8.json"
    },
    "sprite_usage_surface": {
      "$ref": "../split-schema/SpriteUsageSurfaceHint.json"
    },
    "layer_group": {
      "$ref": "../split-schema/TileRenderLayer.json"
    },
    "transition_merges_with_tile": {
      "$ref": "../split-schema/TileID.json"
    },
    "effect_color": {
      "$ref": "../split-schema/Color.json"
    },
    "tint": {
      "$ref": "../split-schema/Color.json"
    },
    "particle_tints": {
      "$ref": "../split-schema/TileBasedParticleTints.json"
    },
    "walking_sound": {
      "$ref": "../split-schema/Sound.json"
    },
    "landing_steps_sound": {
      "$ref": "../split-schema/Sound.json"
    },
    "driving_sound": {
      "$ref": "../split-schema/Sound.json"
    },
    "build_sound": {
      "anyOf": [
        {
          "$ref": "../split-schema/Sound.json"
        },
        {
          "type": "object",
          "properties": {
            "small": {
              "$ref": "../split-schema/Sound.json"
            },
            "medium": {
              "$ref": "../split-schema/Sound.json"
            },
            "large": {
              "$ref": "../split-schema/Sound.json"
            },
            "animated": {
              "$ref": "../split-schema/Sound.json"
            }
          }
        }
      ]
    },
    "mined_sound": {
      "$ref": "../split-schema/Sound.json"
    },
    "walking_speed_modifier": {
      "$ref": "../split-schema/double.json"
    },
    "vehicle_friction_modifier": {
      "$ref": "../split-schema/double.json"
    },
    "decorative_removal_probability": {
      "$ref": "../split-schema/float.json"
    },
    "allowed_neighbors": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/TileID.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "needs_correction": {
      "$ref": "../split-schema/bool.json"
    },
    "minable": {
      "$ref": "../split-schema/MinableProperties.json"
    },
    "fluid": {
      "$ref": "../split-schema/FluidID.json"
    },
    "next_direction": {
      "$ref": "../split-schema/TileID.json"
    },
    "can_be_part_of_blueprint": {
      "$ref": "../split-schema/bool.json"
    },
    "is_foundation": {
      "$ref": "../split-schema/bool.json"
    },
    "destroys_dropped_items": {
      "$ref": "../split-schema/bool.json"
    },
    "allows_being_covered": {
      "$ref": "../split-schema/bool.json"
    },
    "searchable": {
      "$ref": "../split-schema/bool.json"
    },
    "max_health": {
      "$ref": "../split-schema/float.json"
    },
    "weight": {
      "$ref": "../split-schema/Weight.json"
    },
    "dying_explosion": {
      "anyOf": [
        {
          "$ref": "../split-schema/ExplosionDefinition.json"
        },
        {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "../split-schema/ExplosionDefinition.json"
              }
            },
            {
              "type": "object",
              "additionalProperties": false
            }
          ]
        }
      ]
    },
    "absorptions_per_second": {
      "type": "object",
      "additionalProperties": {
        "$ref": "../split-schema/double.json"
      },
      "propertyNames": {
        "$ref": "../split-schema/AirbornePollutantID.json"
      }
    },
    "default_cover_tile": {
      "$ref": "../split-schema/TileID.json"
    },
    "frozen_variant": {
      "$ref": "../split-schema/TileID.json"
    },
    "thawed_variant": {
      "$ref": "../split-schema/TileID.json"
    },
    "effect": {
      "$ref": "../split-schema/TileEffectDefinitionID.json"
    },
    "trigger_effect": {
      "$ref": "../split-schema/TriggerEffect.json"
    },
    "default_destroyed_dropped_item_trigger": {
      "$ref": "../split-schema/Trigger.json"
    },
    "scorch_mark_color": {
      "$ref": "../split-schema/Color.json"
    },
    "check_collision_with_entities": {
      "$ref": "../split-schema/bool.json"
    },
    "effect_color_secondary": {
      "$ref": "../split-schema/Color.json"
    },
    "effect_is_opaque": {
      "$ref": "../split-schema/bool.json"
    },
    "transitions": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/TileTransitionsToTiles.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "transitions_between_transitions": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/TileTransitionsBetweenTransitions.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "autoplace": {
      "$ref": "../split-schema/AutoplaceSpecification.json"
    },
    "placeable_by": {
      "anyOf": [
        {
          "$ref": "../split-schema/ItemToPlace.json"
        },
        {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "../split-schema/ItemToPlace.json"
              }
            },
            {
              "type": "object",
              "additionalProperties": false
            }
          ]
        }
      ]
    },
    "bound_decoratives": {
      "anyOf": [
        {
          "$ref": "../split-schema/DecorativeID.json"
        },
        {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "../split-schema/DecorativeID.json"
              }
            },
            {
              "type": "object",
              "additionalProperties": false
            }
          ]
        }
      ]
    },
    "ambient_sounds_group": {
      "$ref": "../split-schema/TileID.json"
    },
    "ambient_sounds": {
      "anyOf": [
        {
          "$ref": "../split-schema/WorldAmbientSoundDefinition.json"
        },
        {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "../split-schema/WorldAmbientSoundDefinition.json"
              }
            },
            {
              "type": "object",
              "additionalProperties": false
            }
          ]
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "collision_mask",
    "layer",
    "map_color",
    "name",
    "type",
    "variants"
  ]
}