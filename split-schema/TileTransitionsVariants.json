{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/TileTransitionsVariants.html",
  "type": "object",
  "properties": {
    "main": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/TileMainPictures.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "material_texture_width_in_tiles": {
      "$ref": "../split-schema/uint8.json"
    },
    "material_texture_height_in_tiles": {
      "$ref": "../split-schema/uint8.json"
    },
    "material_background": {
      "$ref": "../split-schema/MaterialTextureParameters.json"
    },
    "light": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/TileLightPictures.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "material_light": {
      "$ref": "../split-schema/MaterialTextureParameters.json"
    },
    "empty_transitions": {
      "$ref": "../split-schema/bool.json"
