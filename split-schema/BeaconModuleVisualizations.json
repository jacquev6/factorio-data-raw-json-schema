{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/BeaconModuleVisualizations.html",
  "type": "object",
  "properties": {
    "art_style": {
      "$ref": "../split-schema/string.json"
    },
    "use_for_empty_slots": {
      "$ref": "../split-schema/bool.json"
    },
    "tier_offset": {
      "$ref": "../split-schema/int32.json"
    },
    "slots": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "oneOf": [
              {
                "type": "array",
                "items": {
                  "$ref": "../split-schema/BeaconModuleVisualization.json"
                }
              },
              {
                "type": "object",
                "additionalProperties": false
              }
            ]
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    }
  },
  "required": [
    "art_style"
  ]
}