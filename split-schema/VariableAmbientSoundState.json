{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/VariableAmbientSoundState.html",
  "type": "object",
  "properties": {
    "name": {
      "$ref": "../split-schema/string.json"
    },
    "type": {
      "$ref": "../split-schema/VariableAmbientSoundStateType.json"
    },
    "next_state": {
      "$ref": "../split-schema/string.json"
    },
    "next_states": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/VariableAmbientSoundNextStateItem.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "next_state_trigger": {
      "$ref": "../split-schema/VariableAmbientSoundNextStateTrigger.json"
    },
    "next_state_layers_finished_layers": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/string.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "state_duration_seconds": {
      "$ref": "../split-schema/uint32.json"
    },
    "layers_properties": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/VariableAmbientSoundLayerStateProperties.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "start_pause": {
      "$ref": "../split-schema/RandomRange.json"
    },
    "end_pause": {
      "$ref": "../split-schema/RandomRange.json"
    },
    "number_of_enabled_layers": {
      "$ref": "../split-schema/RandomRange.json"
    }
  },
  "required": [
    "name"
  ]
}