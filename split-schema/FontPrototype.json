{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/prototypes/FontPrototype.html",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "font"
    },
    "name": {
      "$ref": "../split-schema/string.json"
    },
    "size": {
      "$ref": "../split-schema/int32.json"
    },
    "from": {
      "$ref": "../split-schema/string.json"
    },
    "spacing": {
      "$ref": "../split-schema/float.json"
    },
    "border": {
      "$ref": "../split-schema/bool.json"
    },
    "filtered": {
      "$ref": "../split-schema/bool.json"
    },
    "border_color": {
      "$ref": "../split-schema/Color.json"
    }
  },
  "required": [
    "type",
    "name",
    "size",
    "from"
  ]
}