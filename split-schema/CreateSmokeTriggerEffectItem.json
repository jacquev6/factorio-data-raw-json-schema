{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "description": "https://lua-api.factorio.com/stable/types/CreateSmokeTriggerEffectItem.html",
  "type": "object",
  "properties": {
    "repeat_count": {
      "$ref": "../split-schema/double.json"
    },
    "repeat_count_deviation": {
      "$ref": "../split-schema/uint16.json"
    },
    "probability": {
      "$ref": "../split-schema/float.json"
    },
    "affects_target": {
      "$ref": "../split-schema/bool.json"
    },
    "show_in_tooltip": {
      "$ref": "../split-schema/bool.json"
    },
    "damage_type_filters": {
      "$ref": "../split-schema/DamageTypeFilters.json"
    },
    "type": {
      "type": "string",
      "const": "create-smoke"
    },
    "entity_name": {
      "$ref": "../split-schema/EntityID.json"
    },
    "offset_deviation": {
      "$ref": "../split-schema/BoundingBox.json"
    },
    "trigger_created_entity": {
      "$ref": "../split-schema/bool.json"
    },
    "check_buildability": {
      "$ref": "../split-schema/bool.json"
    },
    "tile_collision_mask": {
      "$ref": "../split-schema/CollisionMaskConnector.json"
    },
    "offsets": {
      "oneOf": [
        {
          "type": "array",
          "items": {
            "$ref": "../split-schema/Vector.json"
          }
        },
        {
          "type": "object",
          "additionalProperties": false
        }
      ]
    },
    "as_enemy": {
      "$ref": "../split-schema/bool.json"
    },
    "ignore_no_enemies_mode": {
      "$ref": "../split-schema/bool.json"
    },
    "find_non_colliding_position": {
      "$ref": "../split-schema/bool.json"
    },
    "non_colliding_search_radius": {
      "$ref": "../split-schema/double.json"
    },
    "non_colliding_search_precision": {
      "$ref": "../split-schema/double.json"
    },
    "non_colliding_fail_result": {
      "$ref": "../split-schema/Trigger.json"
    },
    "protected": {
      "$ref": "../split-schema/bool.json"
    },
    "initial_height": {
      "$ref": "../split-schema/float.json"
    },
    "speed": {
      "$ref": "../split-schema/Vector.json"
    },
    "speed_multiplier": {
      "$ref": "../split-schema/float.json"
    },
    "speed_multiplier_deviation": {
      "$ref": "../split-schema/float.json"
    },
    "starting_frame": {
      "$ref": "../split-schema/float.json"
    },
    "starting_frame_deviation": {
      "$ref": "../split-schema/float.json"
    },
    "speed_from_center": {
      "$ref": "../split-schema/float.json"
    },
    "speed_from_center_deviation": {
      "$ref": "../split-schema/float.json"
    }
  },
  "required": [
    "type",
    "entity_name"
  ]
}